import javax.swing.*;
public class prime {
	public static void main(String[] args){
		int n=20;
		for(int i=2;i<=n;i++){
		if(isPrime(i)==true)
		System.out.println("Prime"+i+" ");
		}
	}

}
public static boolean isPrime(int n){
	for(int i=2;i<=n;i++){//求从2到n为止这么多数字中有哪些是 质数
		
		for(int j=2;j<=Math.sqrt(i);j++){
			if((i%j)==0){
				break;}
			}
		if(j>=Math.sqrt(i)){
			return true;
		}else{
			return false;
		}
			
	}
	
}
