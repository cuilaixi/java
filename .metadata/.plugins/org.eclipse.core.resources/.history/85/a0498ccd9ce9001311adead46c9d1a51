import javax.swing.*;
public class prime {
	public static void main(String[] args){
		int n=20;
		for(int i=2;i<=n;i++){
			if(isPrime(i)==true){
				System.out.println("Prime"+i+" ");
			}
		}
	}


	public static boolean isPrime(int n){
		int i;
		for(i=2;i<=Math.sqrt(n);i++){//求从2到n为止这么多数字中有哪些是 质数
				if((n%i)==0){
					return false;
				}	
		}
			return false;
		}
		
}
