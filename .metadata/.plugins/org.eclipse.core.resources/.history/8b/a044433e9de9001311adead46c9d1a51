import javax.swing.*;
public class prime {
	public static void main(String[] args){
		int n=20;
		for(int j=2;j<=n;j++){
			if(isPrime(j)==true){
				System.out.print("Prime"+j+" ");
			}
		}
		System.out.println("are Prime");
	}


	public static boolean isPrime(int n){
		int i;
		for(i=2;i<=Math.sqrt(n);i++){//求从2到n为止这么多数字中有哪些是 质数
				if((n%i)==0){
					return false;
				}	
		}
			return true;
		}
		
}
